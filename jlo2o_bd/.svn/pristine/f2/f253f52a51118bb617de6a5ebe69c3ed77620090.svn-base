.title{ border-bottom:1px solid #eee; line-height:50px;}
.title h2{ font-weight:100; font-size:20px;}
.title ul{ float:right;}
.title ul li i{ margin:0 10px;}

.content{ margin-top:10px;}
.content_left{}
.tabel_title{ height:40px; line-height:39px; background-color:#4d90fe; padding-right:10px;}
.tabel_title h3{ color:#fff; margin-left:20px;}
.tabel_title h3 span{ font-weight:100; margin-left:5px;}
.tabel_title h3.xuan{ background-color:rgba(1,1,1,0.1); }

.tabel_title i{float:right; width:24px; height:24px; line-height:24px; text-align:center; font-size:16px; color:#fff; margin:8px 5px 0 0; cursor:pointer;}
.tabel_title i:hover{ background-color:#0d90fa; background-color:rgba(1,1,1,0.1);}

.tabel_show{ border:1px solid #e1e1e1; overflow-y:auto;}
.tabel_show dl{ width:2000px; height:40px; line-height:39px; font-size:14px; border-bottom:1px solid #e1e1e1; position:relative;}
.tabel_show dt{ height:100%; border-right:1px solid #e1e1e1; width:40px; float:left;}
.tabel_show dt input[type="checkbox"]{ float:left; width:16px; height:16px; margin:12px;}
.tabel_show dd{ float:left; height:100%; border-left:1px solid #e1e1e1; padding:0 10px; margin-left:-1px;}

.tabel_show dl.tabel_nav{ background-color:#f5f5f5 !important; background-image: -webkit-linear-gradient(#f9f9f9,#eeeeee) !important; background-image: -moz-linear-gradient(#f9f9f9,#eeeeee) !important; background-image: -o-linear-gradient(#f9f9f9,#eeeeee) !important; background-image: linear-gradient(#f9f9f9,#eeeeee) !important; font-weight:bold; border-bottom:1px solid #ccc;}
.tabel_show dl.tabel_nav dd{ color:#000;}
.tabel_show dl:hover{ background-color:rgb(241, 248, 253);}
.tabel_show dl.xuan{ background-color:rgb(241, 248, 253);}

.table_control{ height:50px; background-color:#f1f8fd;}
.table_control input[type="button"]{ margin-left:10px;}

ul.jl_paging02{ float:right;}
ul.jl_paging02 li{ float:left; margin:10px 10px 10px 0px;}
ul.jl_paging02 li i{ width:24px; height:24px; line-height:22px; border:1px solid #999; border-radius:50%; cursor:pointer; text-align:center;}
ul.jl_paging02 li span{ line-height:24px;}
ul.jl_paging02 li i:hover{ border-color:rgb(48, 126, 204); color:rgb(48, 126, 204);}
ul.jl_paging02 li input[type="text"]{ width:40px; height:24px; margin:0px; padding:0px; text-align:center; text-indent:0px;}
ul.jl_paging02 li input[type="text"]:hover{ border-color:rgb(48, 126, 204);}
ul.jl_paging02 li input[type="button"]{ height:24px; padding:0px 5px; margin:0px; font-size:12px;}

.content_right{ padding-left:20px;}
.content_right .case{ border:1px solid #ccc; margin-bottom:10px;}
.content_right .case .case_title{ line-height:35px; padding:0 10px; border-bottom:1px solid #ccc; background-color:#f9f9f9;}
.content_right .case .case_title h3{ margin-right:20px; color:#666;}
.content_right .case .case_title h3.xuan{ color:#333}
.content_right .case .case_title a{ float:right;}
.content_right .case ul{}
.content_right .case li{ border-bottom:1px solid #eee; padding:10px;}
.content_right .case li span{ color:#999; font-size:14px;}
.content_right .case li label{ font-size:14px;}
.content_right .case textarea{ padding:5px 10px; max-width:100%; max-height:200px; height:60px;}