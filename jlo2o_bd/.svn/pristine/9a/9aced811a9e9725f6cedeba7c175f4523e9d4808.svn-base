.adv_index_banner ul.banner_nav li span{ position:absolute; width:20px; height:20px; line-height:20px; cursor:pointer; background:#333; color:#fff; left:50%; margin-left:-15px; border-radius:50%; text-align:center;}
.adv_index_banner ul.banner_nav li.xuan span{ background:#e84d1c;}

.adv_index_floor{ margin-top:20px; border:1px solid #eee;}
.adv_index_floor h3{ line-height:30px; padding-left:20px;}
.adv_index_floor .floor_adve{ height:130px; line-height:130px; font-size:40px;}
.adv_index_floor .floor_pro li{ padding:0 0 10px 10px;}
.adv_index_floor .floor_pro li .pro_main{ height:60px; line-height:60px; text-align:center; font-size:30px;}

.adv_index_floor .floor_brand{ margin-top:10px;}
.adv_index_floor .floor_brand li{ padding-left:10px;}
.adv_index_floor .floor_brand li:first-child{ padding:0px;}
.adv_index_floor .floor_brand li .pro_main{ height:30px; line-height:30px; text-align:center; font-size:20px;}

.adv_jg{ padding-left:20px;}
.adv_jg ul{ border:1px solid #eee; width:100%; margin-bottom:10px;}
.adv_jg ul li{ line-height:40px; padding:0 10px; margin-bottom:10px;}
.adv_jg ul li.title{ height:30px; line-height:30px; font-weight:bold; background:#f5f5f5; font-size:14px; border-bottom:1px solid #eee;}
.adv_jg ul li dl.public_table_main dd{ min-height:40px; line-height:40px;}
.adv_jg ul li dl.public_table_main dd i{ color:#d22d29; font-size:18px;}
.adv_jg ul li b{ font-size:18px; color:#d22d29;}


.adv_jg ul li .up_img{ width:50px; height:50px;}
.adv_jg ul li .up_img i{ line-height:40px;}
.adv_jg ul li span{ text-align:right;}
.adv_jg ul li input[type="text"]{ height:30px; margin:5px 0px;}
.adv_jg ul li .adv_case{ background:#d22d29; color:#fff; padding:5px; margin-bottom:10px;}
.adv_jg ul li .adv_case i{ margin-left:20px; font-size:18px; cursor:pointer;}







