.adv_main .color_01:hover{ border:2px solid #cb3302; cursor:pointer;}
.adv_main .color_02:hover{ border:2px solid #cb3302; cursor:auto;}
.adv_main .color_03:hover{ border:2px solid #cb3302; cursor:auto;}

