/*******************首页楼层*****************/
.area{ padding:0 20px;}
.area li{ height:40px; margin-bottom:10px;}
.area li span{ width:30px; height:30px; background:#aaa; border-radius:50%; text-align:center; line-height:30px; color:#fff; font-size:14px; float:left; margin:5px 0; transition:background 0.5s;}
.area li i{ color:#ccc; height:40px; line-height:40px; padding-left:15px; font-size:16px; float:left;}
.area li:hover span{ cursor:pointer; background:#333;}
.area li:hover i{ color:#666;}
.area li.xuan span{ background:#d22d29;}

.adv_main{ border:1px solid #eee; padding:10px;}
.adv_main .color_01{ background:#eee; line-height:100%; text-align:center; border:2px solid #eee; transition:border 0.5s;}
.adv_main .color_02{ background:#fcc065; line-height:100%; text-align:center; border:2px solid #fcc065; transition:border 0.5s;}
.adv_main .color_03{ background:#6df6b4; line-height:100%; text-align:center; border:2px solid #6df6b4; transition:border 0.5s;}

.adv_main .color_01 i{ color:#aaa;}
