ul.xz em{ float:left; width:25px; height:15px; margin:10px 5px;}


.adv_index{ margin:10px 0;}
.adv_header{ height:60px; line-height:60px; font-size:20px; color:#aaa; text-align:center; border:1px solid #ccc;}
.adv_index_banner{ height:120px; margin:10px 0; position:relative;}
.adv_index_banner ul.banner_main{ position:absolute; z-index:1; width:100%; height:120px;}
.adv_index_banner ul.banner_main li{ color:#fff; font-size:20px; text-align:center; line-height:120px; display:none; font-size:50px;}
.adv_index_banner ul.banner_main li.xuan{ display:block;}

.adv_index_banner ul.banner_nav{ position:absolute; z-index:2; width:180px; bottom:10px; height:20px; left:50%; margin-left:-90px;}
